    char_speed = global.spd;
} else if (keyboard_check_released(ord('S'))) {
    char_speed = global.spd;
}

if(keyboard_check(ord('W'))) {
    motion_set(image_angle, char_speed);
} else if (keyboard_check(ord('S'))) {
    motion_set(image_angle, -char_speed);
} else {
    motion_set(image_angle, 0);
    char_speed = global.spd;
}

//Ice Physics
if (global.roomnext &gt; 9 &amp;&amp; global.roomnext &lt; 20) {
    if(instance_number(ice_obj) != 0) {
        close_ice = instance_nearest(player_obj.x, player_obj.y, ice_obj);
        if (player_obj.x &gt;= close_ice.x-60 &amp;&amp; player_obj.x &lt;= close_ice.x  + 60 &amp;&amp;
            player_obj.y &gt;= close_ice.y-60 &amp;&amp; player_obj.y &lt;= close_ice.y + 60) {
                char_speed = 8;
        }
        
        else if((player_obj.x &gt;= 0 &amp;&amp; player_obj.x &lt;= 200) || (player_obj.x &gt;= 600 &amp;&amp; player_obj.x &lt;= 800)) {
            if(char_speed &lt; 8) {
                char_speed += .05;
            }
        }
        else {
            char_speed = global.spd;
        }    
    }
    else if((player_obj.x &gt;= 0 &amp;&amp; player_obj.x &lt;= 200) || (player_obj.x &gt;= 600 &amp;&amp; player_obj.x &lt;= 800)) {
        if(char_speed &lt; 8) {
            char_speed += .05;
        }
    }
    else {
        char_speed = global.spd;
    }
}
if(global.roomnext == 29 &amp;&amp; freeze_timer &gt; 0) {
    freeze_timer -=1;
    global.final_boss_fight_started = true;
}
